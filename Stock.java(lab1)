class Stock{
  
  String symbol;
  String name;
  double prevoiusClosingPrice;
  double currentPrice;
  
  
  Stock(String symbol, String name, double prevoiusClosingPrice, double currentPrice)
  {
    symbol = newSymbol;
    name= newName;
    prevoiusClosingPrice = newPrevoiusClosingPrice ;
    currentPrice = newCurrentPrice;
  }
}
